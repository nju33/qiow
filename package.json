{
  "name": "qiow",
  "version": "0.1.0",
  "author": "nju33 <nju33.ki@gmail.com>",
  "description": "qiita client",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && build",
    "build:dir": "node .electron-vue/build.js && build --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "test": "NODE_ENV=test ava"
  },
  "build": {
    "productName": "qiow",
    "appId": "qiow",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "camelcase-keys": "^4.1.0",
    "cheerio": "^1.0.0-rc.1",
    "electron-json-storage": "^3.0.5",
    "escape-html": "^1.0.3",
    "highlightjs": "^9.10.0",
    "immutable": "^3.8.2",
    "intersection-observer": "^0.3.2",
    "jss": "^7.1.5",
    "jss-preset-default": "^2.0.0",
    "jump.js": "^1.0.2",
    "lodash-decorators": "^4.3.5",
    "moveto": "^1.6.1",
    "pify": "^3.0.0",
    "prettier": "^1.5.2",
    "query-string": "^4.3.4",
    "ramda": "^0.24.1",
    "rxjs": "^5.4.0",
    "smoothscroll-polyfill": "^0.3.5",
    "snakecase-keys": "^1.1.0",
    "timeago.js": "^3.0.2",
    "tippy.js": "^1.0.0",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-lazyload": "^1.0.5",
    "vue-octicon": "^2.1.1",
    "vue-prdiv": "^0.1.0",
    "vue-router": "^2.5.3",
    "vue-rx": "^3.2.0",
    "vuex": "^2.3.1",
    "zenscroll": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.7.9",
    "electron-builder": "^18.1.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.1.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.4",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
